            <Label text="Street" GridPane.rowIndex="6" />
            <Label text="State" GridPane.rowIndex="7" />
            <Label text="City" GridPane.rowIndex="8" />
            <Label text="Zip" GridPane.rowIndex="9" />
            <TextField fx:id="firstName" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
